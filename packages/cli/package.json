{
  "name": "@nutmeg/cli",
  "version": "0.17.2",
  "publishConfig": {
    "access": "public"
  },
  "description": "Build, test, and publish vanilla Web Components with a little spice",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/abraham/nutmeg.git"
  },
  "author": {
    "name": "Abraham Williams",
    "email": "abraham@abrah.am",
    "url": "https://abrah.am"
  },
  "homepage": "https://github.com/abraham/nutmeg",
  "bugs": {
    "url": "https://github.com/abraham/nutmeg/issues"
  },
  "main": "dist/cli.js",
  "bin": {
    "nutmeg-build": "bin/nutmeg-build",
    "nutmeg-clean": "bin/nutmeg-clean",
    "nutmeg-new": "bin/nutmeg-new",
    "nutmeg-serve": "bin/nutmeg-serve",
    "nutmeg-test": "bin/nutmeg-test",
    "nutmeg-watch": "bin/nutmeg-watch",
    "nutmeg": "bin/nutmeg"
  },
  "engines": {
    "node": "14 || 16 || 18"
  },
  "scripts": {
    "build": "tsc",
    "clean": "rimraf dist",
    "prebuild": "npm run clean",
    "prepare": "npm run build",
    "test": "npm run build",
    "watch": "tsc --watch",
    "xtest": "ts-node ./scripts/test.ts"
  },
  "keywords": [
    "web-components",
    "webcomponents",
    "shadow-dom",
    "shadowdom",
    "lit-html",
    "nutmeg",
    "typescript"
  ],
  "dependencies": {
    "@nutmeg/seed": "0.17.2",
    "@types/chai": "^4.0.0",
    "@types/mocha": "^8.0.0",
    "@types/sinon": "^10.0.0",
    "babel-preset-env": "^1.0.0",
    "chai": "^4.0.0",
    "commander": "^9.0.0",
    "karma": "^6.0.0",
    "karma-chai": "^0.1.0",
    "karma-chrome-launcher": "^3.0.0",
    "karma-firefox-launcher": "^2.0.0",
    "karma-mocha": "^2.0.0",
    "karma-opera-launcher": "^1.0.0",
    "karma-safari-launcher": "^1.0.0",
    "karma-sinon": "^1.0.0",
    "karma-typescript": "^5.0.0",
    "karma-typescript-es6-transform": "^5.0.0",
    "lodash.template": "^4.0.0",
    "mocha": "8.1.3",
    "pascal-case": "^3.0.0",
    "recursive-copy": "^2.0.0",
    "shelljs": "^0.8.0",
    "sinon": "^14.0.0",
    "through2": "^4.0.0",
    "ts-loader": "^9.0.0",
    "typescript": "4.2.4",
    "update-notifier": "^5.0.0"
  },
  "devDependencies": {
    "@types/lodash.template": "^4.0.0",
    "@types/node": "^16.0.0",
    "@types/shelljs": "^0.8.0",
    "@types/through2": "^2.0.0",
    "@types/update-notifier": "^5.0.0",
    "rimraf": "^3.0.0",
    "ts-node": "^10.0.0"
  },
  "types": "dist/cli.d.ts",
  "browser": {
    "fs": false,
    "child_process": false
  },
  "directories": {
    "test": "test"
  }
