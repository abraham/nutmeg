  "dependencies": {
    "@nutmeg/seed": "^0.17.2",
    "@types/chai": "4.2.22",
    "@types/mocha": "8.0.3",
    "@types/sinon": "10.0.6",
    "@webcomponents/webcomponentsjs": "2.6.0",
    "babel-preset-env": "1.7.0",
    "chai": "4.3.4",
    "commander": "8.3.0",
    "html-webpack-plugin": "5.5.0",
    "karma": "6.3.9",
    "karma-chai": "0.1.0",
    "karma-chrome-launcher": "3.1.0",
    "karma-firefox-launcher": "2.1.2",
    "karma-mocha": "2.0.1",
    "karma-opera-launcher": "1.0.0",
    "karma-safari-launcher": "1.0.0",
    "karma-sinon": "1.0.5",
    "karma-typescript": "5.5.2",
    "karma-typescript-es6-transform": "5.5.2",
    "karma-webpack": "5.0.0",
    "lodash.template": "4.5.0",
    "mocha": "8.1.3",
    "pascal-case": "3.1.2",
    "recursive-copy": "2.0.13",
    "shelljs": "0.8.4",
    "sinon": "12.0.1",
    "through2": "4.0.2",
    "ts-loader": "9.2.6",
    "typescript": "4.2.4",
    "update-notifier": "5.1.0",
    "webpack": "5.64.4",
    "webpack-bundle-analyzer": "4.5.0",
    "webpack-cli": "4.9.1",
    "webpack-dev-server": "4.6.0"
  },
  "devDependencies": {
    "@types/lodash.template": "4.5.0",
    "@types/node": "16.11.10",
    "@types/shelljs": "0.8.9",
    "@types/through2": "2.0.36",
    "@types/update-notifier": "5.1.0",
    "rimraf": "3.0.2",
    "ts-node": "10.4.0"
  },
  "types": "dist/cli.d.ts",
  "browser": {
    "fs": false,
    "child_process": false
  },
  "directories": {
    "test": "test"
  }
