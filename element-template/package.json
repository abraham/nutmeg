{
  "name": "<%= tag %>",
  "version": "0.1.0",
  "description": "Generated Nutmeg Web Component",
  "main": "dist/<%= tag %>.js",
  "module": "dist/<%= tag %>.js",
  "unpkg": "dist/<%= tag %>.min.js",
  "types": "dist/<%= tag %>.d.ts",
  "directories": {
    "test": "test"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "keywords": [
    "<%= tag %>",
    "nutmeg"
  ],
  "license": "MIT",
  "scripts": {
    "build": "tsc && webpack",
    "clean": "rm -rf dist/*",
    "prebuild": "npm run clean",
    "prepare": "npm run build",
    "pretest": "npm run build",
    "serve": "npm run watch & http-server",
    "test": "karma start",
    "watch": "tsc --watch & webpack --watch"
  },
  "dependencies": {
    "lit-html": "^0.7.0"
  },
  "devDependencies": {
    "@types/chai": "4.0.8",
    "@types/karma-fixture": "0.2.3",
    "@types/mocha": "2.2.44",
    "@webcomponents/webcomponentsjs": "1.0.20",
    "chai": "4.1.2",
    "http-server": "0.10.0",
    "karma": "1.7.1",
    "karma-chai": "0.1.0",
    "karma-chrome-launcher": "2.2.0",
    "karma-firefox-launcher": "1.1.0",
    "karma-fixture": "0.2.6",
    "karma-html2js-preprocessor": "1.1.0",
    "karma-mocha": "1.3.0",
    "karma-opera-launcher": "1.0.0",
    "karma-safari-launcher": "1.0.0",
    "karma-typescript": "3.0.8",
    "karma-webpack": "2.0.6",
    "mocha": "4.0.1",
    "typescript": "2.6.2",
    "uglifyjs-webpack-plugin": "1.1.2",
    "webpack": "3.10.0"
  }
}
