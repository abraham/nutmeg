{
  "name": "<%= tag %>",
  "version": "0.1.0",
  "description": "Generated Nutmeg Web Component",
  "main": "dist/<%= tag %>.js",
  "module": "dist/<%= tag %>.js",
  "types": "dist/<%= tag %>.d.ts",
  "directories": {
    "test": "test"
  },
  "engines": {
    "node": "16"
  },
  "keywords": [
    "<%= tag %>",
    "nutmeg"
  ],
  "license": "MIT",
  "scripts": {
    "build": "nutmeg build .",
    "prebuild": "nutmeg clean .",
    "prepare": "npm run build -- --production",
    "pretest": "npm run build",
    "start": "nutmeg serve .",
    "test": "nutmeg test .",
    "watch": "nutmeg watch ."
  },
  "dependencies": {
    "@nutmeg/seed": "<%= seedSource %>"
  },
  "devDependencies": {
    "@nutmeg/cli": "<%= cliSource %>"
  }
}
