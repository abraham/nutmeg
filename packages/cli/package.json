{
  "name": "@nutmeg/cli",
  "version": "0.17.2",
  "publishConfig": {
    "access": "public"
  },
  "description": "Build, test, and publish vanilla Web Components with a little spice",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/abraham/nutmeg.git"
  },
  "author": {
    "name": "Abraham Williams",
    "email": "abraham@abrah.am",
    "url": "https://abrah.am"
  },
  "homepage": "https://github.com/abraham/nutmeg",
  "bugs": {
    "url": "https://github.com/abraham/nutmeg/issues"
  },
  "main": "dist/cli.js",
  "bin": {
    "nutmeg-build": "bin/nutmeg-build",
    "nutmeg-clean": "bin/nutmeg-clean",
    "nutmeg-new": "bin/nutmeg-new",
    "nutmeg-serve": "bin/nutmeg-serve",
    "nutmeg-test": "bin/nutmeg-test",
    "nutmeg-watch": "bin/nutmeg-watch",
    "nutmeg": "bin/nutmeg"
  },
  "engines": {
    "node": "16"
  },
  "scripts": {
    "build": "tsc",
    "clean": "rimraf dist",
    "prebuild": "npm run clean",
    "prepare": "npm run build",
    "test": "npm run build",
    "watch": "tsc --watch",
    "xtest": "ts-node ./scripts/test.ts"
  },
  "keywords": [
    "web-components",
    "webcomponents",
    "shadow-dom",
    "shadowdom",
    "lit-html",
    "nutmeg",
    "typescript"
  ],
  "dependencies": {
    "@nutmeg/seed": "^0.17.2",
    "babel-preset-env": "1.7.0",
    "commander": "9.0.0",
    "happy-dom": "^2.45.0",
    "lodash.template": "4.5.0",
    "pascal-case": "3.1.2",
    "recursive-copy": "2.0.14",
    "shelljs": "0.8.5",
    "through2": "4.0.2",
    "ts-loader": "9.2.7",
    "typescript": "4.2.4",
    "update-notifier": "5.1.0",
    "vitest": "^0.5.9"
  },
  "devDependencies": {
    "@types/lodash.template": "4.5.0",
    "@types/node": "16.11.26",
    "@types/shelljs": "0.8.11",
    "@types/through2": "2.0.36",
    "@types/update-notifier": "5.1.0",
    "rimraf": "3.0.2",
    "ts-node": "10.7.0"
  },
  "types": "dist/cli.d.ts",
  "browser": {
    "fs": false,
    "child_process": false
  },
  "directories": {
    "test": "test"
  }
}
