{
  "name": "@nutmeg/cli",
  "version": "0.17.2",
  "description": "Build, test, and publish vanilla Web Components with a little spice",
  "main": "dist/cli.js",
  "types": "dist/cli.d.ts",
  "bin": {
    "nutmeg-build": "bin/nutmeg-build",
    "nutmeg-clean": "bin/nutmeg-clean",
    "nutmeg-new": "bin/nutmeg-new",
    "nutmeg-serve": "bin/nutmeg-serve",
    "nutmeg-test": "bin/nutmeg-test",
    "nutmeg-watch": "bin/nutmeg-watch",
    "nutmeg": "bin/nutmeg"
  },
  "directories": {
    "test": "test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/abraham/nutmeg.git"
  },
  "keywords": [
    "web-components",
    "webcomponents",
    "shadow-dom",
    "shadowdom",
    "lit-html",
    "nutmeg",
    "typescript"
  ],
  "author": {
    "name": "Abraham Williams",
    "email": "abraham@abrah.am",
    "url": "https://abrah.am"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/abraham/nutmeg/issues"
  },
  "homepage": "https://github.com/abraham/nutmeg",
  "scripts": {
    "prebuild": "npm run clean",
    "build": "tsc",
    "clean": "rimraf dist",
    "prepare": "npm run build",
    "test": "npm run build",
    "xtest": "ts-node ./scripts/test.ts",
    "watch": "tsc --watch"
  },
  "engines": {
    "node": "12"
  },
  "dependencies": {
    "@nutmeg/seed": "^0.17.2",
    "@types/chai": "4.2.11",
    "@types/mocha": "7.0.1",
    "@types/sinon": "9.0.4",
    "@webcomponents/webcomponentsjs": "2.4.3",
    "babel-preset-env": "1.7.0",
    "chai": "4.2.0",
    "commander": "5.1.0",
    "html-webpack-plugin": "4.3.0",
    "karma": "5.1.0",
    "karma-chai": "0.1.0",
    "karma-chrome-launcher": "3.1.0",
    "karma-firefox-launcher": "1.3.0",
    "karma-mocha": "2.0.1",
    "karma-opera-launcher": "1.0.0",
    "karma-safari-launcher": "1.0.0",
    "karma-sinon": "1.0.5",
    "karma-typescript": "5.0.3",
    "karma-typescript-es6-transform": "5.0.3",
    "karma-webpack": "4.0.2",
    "lodash.template": "4.5.0",
    "mocha": "7.0.1",
    "pascal-case": "3.1.1",
    "recursive-copy": "2.0.10",
    "shelljs": "0.8.4",
    "sinon": "9.0.2",
    "through2": "4.0.2",
    "ts-loader": "7.0.5",
    "typescript": "3.9.6",
    "uglifyjs-webpack-plugin": "2.2.0",
    "update-notifier": "4.1.0",
    "webpack": "4.43.0",
    "webpack-bundle-analyzer": "3.8.0",
    "webpack-cli": "3.3.12",
    "webpack-dev-server": "3.11.0",
    "webpack-stylish": "0.1.8"
  },
  "devDependencies": {
    "@types/lodash.template": "4.5.0",
    "@types/node": "10.17.26",
    "@types/shelljs": "0.8.8",
    "@types/through2": "2.0.36",
    "@types/update-notifier": "4.1.0",
    "rimraf": "3.0.2",
    "ts-node": "8.10.2"
